df["INSTITUCIÓN"] = df["INSTITUCIÓN"].str.upper().str.strip().replace(university_mapping)

# Contar la cantidad de proyectos por institución después de la unificación de nombres
proyectos_por_institucion = df["INSTITUCIÓN"].value_counts().sort_values()

# Configurar el estilo del fondo oscuro
plt.style.use("dark_background")

# Configurar el tamaño de la figura
fig, ax = plt.subplots(figsize=(15, 25))

# Obtener colores de la paleta viridis
norm = mcolors.Normalize(vmin=0, vmax=len(proyectos_por_institucion))
colors = cm.viridis(norm(range(len(proyectos_por_institucion))))

# Graficar con barras horizontales
bars = ax.barh(proyectos_por_institucion.index, proyectos_por_institucion.values, color=colors)

# Etiquetas
title = "Cantidad de Proyectos por Institución (Nombres Unificados)"
ax.set_xlabel("Cantidad de Proyectos", color="white")
ax.set_ylabel("Institución", color="white")
ax.set_title(title, color="white")

# Agregar números sobre las barras
for bar in bars:
    ax.text(bar.get_width() + 1, bar.get_y() + bar.get_height()/2, str(int(bar.get_width())),
            va='center', ha='left', color='white', fontsize=10)

# Ajustar etiquetas del eje Y
plt.yticks(color="white", fontsize=12)
plt.xticks(color="white")
plt.grid(axis="x", linestyle="--", alpha=0.7, color="gray")

# Guardar el gráfico
plt.tight_layout()
plt.show()
